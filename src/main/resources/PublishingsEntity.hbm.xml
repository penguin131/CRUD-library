<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="myApp.model.PublishingEntity" table="Publishing" schema="DB_BOOKS">
        <id name="publishingId">
            <column name="publishingId" sql-type="int(10)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(255)"/>
        </property>
    </class>
</hibernate-mapping>
